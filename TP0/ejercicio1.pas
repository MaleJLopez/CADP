//Lea por teclado dos números enteros e imprima en pantalla los
//valores leídos en orden inverso.

program ejercicio1;
var
	num1, num2: integer;
begin
	writeln('Ingrese primer numero:');
	readln(num1);
	writeln('Ingreser segundo numero:');
	readln(num2);
	writeln('Se ingresaron los numeros ', num2, ' y ', num1);
end.
